{
  "Usage: prog -v": [
    {
      "input": "prog -v",
      "expected": {
        "-v": true
      }
    }
  ],
  "usage: prog [go]": [
    {
      "input": "prog go",
      "expected": {
        "go": true
      }
    }
  ],
  "usage: prog [go go]": [
    {
      "input": "prog",
      "expected": {
        "go": 0
      }
    },
    {
      "input": "prog go",
      "expected": {
        "go": 1
      }
    },
    {
      "input": "prog go go",
      "expected": {
        "go": 2
      }
    },
    {
      "input": "prog go go go",
      "expected": "user-error"
    }
  ],
  "usage: prog go...": [
    {
      "input": "prog go go go go go",
      "expected": {
        "go": 5
      }
    }
  ],
  "usage: prog (go <direction> --speed=<km/h>)...": [
    {
      "input": "prog go left --speed=5 go right --speed=9",
      "expected": {
        "go": 2,
        "<direction>": [
          "left",
          "right"
        ],
        "--speed": [
          "5",
          "9"
        ]
      }
    }
  ]
}
