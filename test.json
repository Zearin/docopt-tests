          }
        },
        {
          "input": "prog --hello wrld",
          "expected": {
            "--hello": "wrld"
          }
        }
      ]
    },
    {
      "doc": "usage: prog [-o]",
      "tests": [
        {
          "input": "prog",
          "expected": {
            "-o": false
          }
        },
        {
          "input": "prog -o",
          "expected": {
            "-o": true
          }
        }
      ]
    },
    {
      "doc": "usage: prog [-opr]",
      "tests": [
        {
          "input": "prog -op",
          "expected": {
            "-o": true,
            "-p": true,
            "-r": false
          }
        }
      ]
    },
    {
      "doc": "usage: prog --aabb | --aa",
      "tests": [
        {
          "input": "prog --aa",
          "expected": {
            "--aabb": false,
            "--aa": true
          }
        },
        {
          "input": "prog --a",
          "expected": "user-error"
        }
      ]
    },
    {
      "doc": "Usage: prog -v",
      "tests": [
        {
          "input": "prog -v",
          "expected": {
            "-v": true
          }
        }
      ]
    },
    {
      "doc": "Usage: prog [-v -v]",
      "tests": [
        {
          "input": "prog",
          "expected": {
            "-v": 0
          }
        },
        {
          "input": "prog -v",
          "expected": {
            "-v": 1
          }
        },
        {
          "input": "prog -vv",
          "expected": {
            "-v": 2
          }
        }
      ]
    },
    {
      "doc": "Usage: prog -v ...",
      "tests": [
        {
          "input": "prog",
          "expected": "user-error"
        },
        {
          "input": "prog -v",
          "expected": {
            "-v": 1
          }
        },
        {
          "input": "prog -vv",
          "expected": {
            "-v": 2
          }
        },
        {
          "input": "prog -vvvvvv",
          "expected": {
            "-v": 6
          }
        }
      ]
    },
    {
      "doc": "Usage: prog [-v | -vv | -vvv]\n\nThis one is probably most readable user-friednly variant.\n",
      "tests": [
        {
          "input": "prog",
          "expected": {
            "-v": 0
          }
        },
        {
          "input": "prog -v",
          "expected": {
            "-v": 1
          }
        },
        {
          "input": "prog -vv",
          "expected": {
            "-v": 2
          }
        },
        {
          "input": "prog -vvvv",
          "expected": "user-error"
        }
      ]
    },
    {
      "doc": "usage: prog [--ver --ver]",
      "tests": [
        {
          "input": "prog --ver --ver",
          "expected": {
            "--ver": 2
          }
        }
      ]
    },
    {
      "doc": "usage: prog [go]",
      "tests": [
        {
          "input": "prog go",
          "expected": {
            "go": true
          }
        }
      ]
    },
    {
      "doc": "usage: prog [go go]",
      "tests": [
        {
          "input": "prog",
          "expected": {
            "go": 0
          }
        },
        {
          "input": "prog go",
          "expected": {
            "go": 1
          }
        },
        {
          "input": "prog go go",
          "expected": {
            "go": 2
          }
        },
        {
          "input": "prog go go go",
          "expected": "user-error"
        }
      ]
    },
    {
      "doc": "usage: prog go...",
      "tests": [
        {
          "input": "prog go go go go go",
          "expected": {
            "go": 5
          }
        }
      ]
    },
    {
      "doc": "usage: prog [options] [-a]\n\noptions: -a\n         -b",
      "tests": [
        {
          "input": "prog -a",
          "expected": {
            "-a": true,
            "-b": false
          }
        },
        {
          "input": "prog -aa",
          "expected": "user-error"
        }
      ]
    },
    {
      "doc": "Usage: prog [options] A\nOptions:\n  -q  Be quiet\n  -v  Be verbose.",
      "tests": [
        {
          "input": "prog arg",
          "expected": {
            "A": "arg",
            "-v": false,
            "-q": false
          }
        },
        {
          "input": "prog -v arg",
          "expected": {
            "A": "arg",
            "-v": true,
            "-q": false
          }
        },
        {
          "input": "prog -q arg",
          "expected": {
            "A": "arg",
            "-v": false,
            "-q": true
          }
        }
      ]
    },
    {
      "doc": "usage: prog [-]",
      "tests": [
        {
          "input": "prog -",
          "expected": {
            "-": true
          }
        },
        {
          "input": "prog",
          "expected": {
            "-": false
          }
        }
      ]
    },
    {
      "doc": "usage: prog [NAME [NAME ...]]",
      "tests": [
        {
          "input": "prog a b",
