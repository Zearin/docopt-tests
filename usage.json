{
  "Usage": {
    "__desc": "In order for Docopt\nAs a command line interpreter\nI want it to understand \"usage\" documentation",
    "empty usage pattern": {
      "usage: prog": [
        {
          "input": "prog",
          "expected": {}
        }
      ]
    },
    "Usage-section syntax": {
      "usage:prog --foo": [
        {
          "input": "prog --foo",
          "expected": {
            "--foo": true
          }
        }
      ],
      "PROGRAM USAGE: prog --foo": [
        {
          "input": "prog --foo",
          "expected": {
            "--foo": true
          }
        }
      ],
      "Usage: prog --foo\n   prog --bar\nNOT PART OF SECTION": [
        {
          "input": "prog --foo",
          "expected": {
            "--foo": true,
            "--bar": false
          }
        }
      ],
      "Usage:\n  prog --foo\n  prog --bar\n\nNOT PART OF SECTION": [
        {
          "input": "prog --foo",
          "expected": {
            "--foo": true,
            "--bar": false
          }
        }
      ],
      "Usage:\n  prog --foo\n  prog --bar\nNOT PART OF SECTION": [
        {
          "input": "prog --foo",
          "expected": {
            "--foo": true,
            "--bar": false
          }
        }
      ]
    }
  }
}
